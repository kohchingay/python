        "def factorial():\n",
        "    n = int(input(\"Enter an integer:\"))\n",
        "    initial=n\n",
        "    result=n\n",
        "    for i in range(n-2):\n",
        "        result=result*(n-1)\n",
        "        n-=1\n",
        "        print(result)\n",
        "    print(f\"\\nThe factorial of {initial} is {result}.\")\n",
        "\n",
        "factorial()"
